{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-unused-vars": [0, { 
    "vars": "local",
    "args": "none" 
  }],
  "no-undef": 0,
  "no-console": 0,
  "semi": [0],
  "no-multiple-empty-lines": [0, {"max": 100}]
  }
}